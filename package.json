{
    "name": "@nomadreservations/ngx-codemirror",
    "version": "1.0.0-rc.3",
    "description": "@nomadreservations/ngx-codemirror CodeMirror (5.x) code editor in your Angular application. Server Side Rendering(SSR) compliant and @angular 5+ Ready.",
    "main": "./bundles/ngx-codemirror.umd.js",
    "module": "./esm5/ngx-codemirror.js",
    "es2015": "./esm2015/ngx-codemirror.js",
    "scripts": {
        "build": "node build.js",
        "build:watch": "nodemon -e js,ts,json --ignore dist --ignore documentation --ignore demo -x npm run build",
        "build:release": "npm-run-all -s doctoc build compodoc",
        "test": "karma start",
        "pack-and-build": "npm-run-all -s build pack-lib",
        "pack:watch": "nodemon -e js,ts,json --ignore dist --ignore documentation --ignore demo -x npm run pack-and-build",
        "pack-lib": "npm pack ./dist",
        "publish-lib": "npm publish ./dist --registry=https://registry.npmjs.org/",
        "publish-lib:next": "npm publish --tag next ./dist --registry=https://registry.npmjs.org/",
        "commit": "git cz -a",
        "release": "release-it",
        "compodoc": "compodoc -p ./tsconfig.json",
        "compodoc:serve": "npm run compodoc -s",
        "compodoc:watch": "nodemon -V -e ts,json,md --ignore dist --ignore documentation --ignore demo -d 5 -x run-s compodoc",
        "docs": "npm-run-all -s compodoc docs:watch",
        "docs:watch": "npm-run-all -p compodoc:serve compodoc:watch",
        "docs:publish": "bash deploy-docs.sh",
        "doctoc": "doctoc README.md --maxlevel 3",
        "watch": "npm-run-all -p build:watch docs",
        "postversion": "git push && git push --tags",
        "demo": "run-s demo:install demo:serve",
        "demo:watch": "cd demo && yarn start",
        "demo:serve": "run-p demo:watch",
        "demo:install": "cd demo && yarn install",
        "demo:aot:yalc": "run-s demo:yalc && cd demo && ng build --prod",
        "demo:watch:aot:yalc": "nodemon -e ts -w src -w demo -w public_api.ts -x run-s demo:aot:yalc",
        "demo:watch:yalc": "nodemon -e ts -w src -w public_api.ts -d 1 -x run-s demo:yalc",
        "demo:yalc": "run-s build && yalc publish dist && cd demo && yalc link @nomadreservations/ngx-codemirror"
    },
    "typings": "./ngx-codemirror.d.ts",
    "homepage": "http://nomadreservations.co",
    "author": {
        "name": "Paul Ryan <paullryan>",
        "url": "https://github.com/paullryan"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nomadreservations/ngx-codemirror"
    },
    "license": "UNLICENSED",
    "keywords": [
        "angular",
        "aot",
        "aot-compatible",
        "aot-compilation",
        "library",
        "ng",
        "ngx-codemirror",
        "typescript"
    ],
    "dependencies": {
        "tslib": "^1.8.1"
    },
    "peerDependencies": {
        "@angular/compiler": "^5.0.0",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/core": "^5.0.0",
        "@angular/forms": "^5.0.0",
        "codemirror": "^5.33.0"
    },
    "devDependencies": {
        "@angular/animations": "5.1.2",
        "@angular/common": "5.1.2",
        "@angular/compiler": "5.1.2",
        "@angular/compiler-cli": "5.1.2",
        "@angular/core": "5.1.2",
        "@angular/forms": "5.1.2",
        "@angular/platform-browser": "5.1.2",
        "@angular/platform-browser-dynamic": "5.1.2",
        "@angular/platform-server": "5.1.2",
        "@compodoc/compodoc": "1.0.5",
        "@types/codemirror": "^0.0.55",
        "@types/jasmine": "2.8.3",
        "@types/node": "^8.5.2",
        "angular2-inline-template-style": "^1.3.0",
        "chalk": "2.3.0",
        "clean-css": "^4.1.9",
        "codelyzer": "4.0.2",
        "codemirror": "^5.33.0",
        "colors": "1.1.2",
        "core-js": "2.5.3",
        "cpx": "^1.5.0",
        "cz-conventional-changelog": "^2.1.0",
        "doctoc": "^1.3.0",
        "html-minifier": "^3.5.8",
        "istanbul-instrumenter-loader": "3.0.0",
        "jasmine-core": "2.8.0",
        "karma": "2.0.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "1.3.3",
        "karma-jasmine": "1.1.1",
        "karma-sourcemap-loader": "0.3.7",
        "karma-spec-reporter": "0.0.32",
        "karma-webpack": "2.0.9",
        "node-sass": "^4.7.2",
        "nodemon": "^1.14.7",
        "npm-run-all": "^4.1.2",
        "reflect-metadata": "0.1.10",
        "release-it": "^5.1.2",
        "replace": "^0.3.0",
        "rimraf": "^2.6.2",
        "rollup": "0.53.3",
        "rollup-globals-regex": "^0.0.3",
        "rollup-plugin-license": "0.5.0",
        "rollup-plugin-node-resolve": "3.0.0",
        "rollup-plugin-sourcemaps": "0.4.2",
        "rxjs": "5.5.6",
        "shelljs": "0.7.8",
        "source-map-loader": "0.2.3",
        "ts-loader": "3.2.0",
        "tslint": "5.8.0",
        "typescript": "~2.5.3",
        "uglify-js": "3.3.4",
        "webpack": "3.10.0",
        "yalc": "^1.0.0-pre.16",
        "yarn": "^1.3.2",
        "zone.js": "0.8.19"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
